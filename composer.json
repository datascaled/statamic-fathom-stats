{
    "name": "datascaled/fathom-stats",
    "autoload": {
        "psr-4": {
            "Datascaled\\FathomStats\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Datascaled\\FathomStats\\Tests\\": "tests/"
        }
    },
    "extra": {
        "statamic": {
            "name": "Statamic Fathom Stats",
            "description": "Statamic Fathom Stats addon"
        },
        "laravel": {
            "providers": [
                "Datascaled\\FathomStats\\ServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.0",
        "statamic/cms": "^4.0"
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true
        }
    },
    "require-dev": {
        "orchestra/testbench": "^8.20",
        "phpunit/phpunit": "^10.5"
    }
}
